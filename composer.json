{
    "name": "aoe/extbase-functionals",
    "description": "Testing-Framework for Extbase-Extensions to easily manage functional controller tests.",
    "type": "typo3-cms-extension",
    "homepage": "https://github.com/AOEpeople/extbase_functionals",
    "license": "GPL-3.0",
    "support": {
        "email": "dev@aoe.com",
        "issues": "https://github.com/AOEpeople/extbase_functionals/issues",
        "source": "https://github.com/AOEpeople/extbase_functionals"
    },
    "authors": [
        {
            "name": "AOE GmbH",
            "email": "dev@aoe.com",
            "homepage": "http://www.aoe.com"
        }
    ],
    "config": {
        "secure-http": false
    },
    "require": {
        "typo3/cms-core": ">=6.2.0,<=7.6.99",
        "typo3/cms-extbase": "*"
    },
    "require-dev": {
        "typo3/cms": "^7.6"
    },
    "autoload": {
        "psr-4": {
            "Aoe\\ExtbaseFunctionals\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TYPO3\\CMS\\Core\\Tests\\": "typo3/sysext/core/Tests/"
        }
    },
    "replace": {
        "extbase_functionals": "*"
    },
    "keywords": [
        "typo3",
        "extbase",
        "unit",
        "functional"
    ]
}